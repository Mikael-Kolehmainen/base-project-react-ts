{
  "include": ["index.ts", "knexfile.ts", "controllers/**/*.ts", "models/**/*.ts"],
  "exclude": ["node_modules"],
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  }
}
